{
  "name": "stars-blog",
  "version": "0.1.0",
  "describe": "tangsx的个人博客",
  "author": "stars <whalefall0@163.com>",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:preview": "vue-cli-service build --mode preview",
    "globle": "npm i -g rimraf npm-check-updates nrm increase-memory-limit cnpm",
    "update": "ncu&&ncu -u&&npm i",
    "rimraf": "rimraf node_modules&&npm i",
    "increase-memory-limit": "increase-memory-limit",
    "inspect": "vue-cli-service inspect --mode development >> webpack.config.development.js",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "core-js": "^3.8.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0",
    "axios": "^0.21.1",
    "qs": "^6.9.4",
    "h_ui": "^1.10.0",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "fingerprintjs": "^0.5.3",
    "clipboard": "^2.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-plugin-router": "~4.5.9",
    "@vue/cli-plugin-vuex": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.4.0",
    "prettier": "^2.2.1",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "^2.6.12",
    "svg-sprite-loader": "^5.2.1",
    "svgo": "^1.3.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "compression-webpack-plugin": "^7.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jquery": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "vue/comment-directive": 0,
      "no-unused-vars": [
        0,
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-invalid-regexp": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
